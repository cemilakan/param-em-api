{
    "name": "param/em-api",
    "description": "Laravel için Param EM API entegrasyon paketi. Token alma ve EFT işlemleri gibi servisleri içerir.",
    "keywords": ["param", "eft", "transfer", "api", "laravel", "entegrasyon"],
    "license": "MIT",
    "homepage": "https://github.com/cemilakan/param-em-api",
    "authors": [
        {
            "name": "Cemil AKAN",
            "email": "cemilakan47@gmail.com",
            "homepage": "https://github.com/cemilakan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Param\\EmApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Param\\EmApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Param\\EmApi\\ParamEmApiServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "prefer-stable": true
}
